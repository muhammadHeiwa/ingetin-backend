# Server Configuration
NODE_ENV=development
PORT=3001

# CORS Configuration
CORS_ORIGIN=*

# Database Configuration
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=
DB_NAME=ingetin_db

# JWT Configuration
JWT_SECRET=JWTS3CR3TKEYEXAMPLE1234567890
JWT_EXPIRES_IN=7d

# Cron Schedule Configuration
